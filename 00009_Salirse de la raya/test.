check_head_position: true

subject: SalirDeLineaVerde

examples:
 - arguments:
   - Este
   initial_board: |
     GBB/1.0
     size 5 2
     cell 0 0 Verde 1
     cell 1 0 Verde 1
     cell 2 0 Verde 1
     head 0 0
   final_board: |
     GBB/1.0
     size 5 2
     cell 0 0 Verde 1
     cell 1 0 Verde 1
     cell 2 0 Verde 1
     head 3 0
     
 - arguments:
   - Sur
   initial_board: |
     GBB/1.0
     size 3 4
     cell 1 3 Verde 1
     head 1 3
   final_board: |
     size 3 4
     cell 1 3 Verde 1
     head 1 2